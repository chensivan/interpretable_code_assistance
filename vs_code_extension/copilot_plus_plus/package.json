{
	"name": "helloworld",
	"displayName": "copilot_plus_plus",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:visual.openVisual"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "visual.openVisual",
				"title": "Open Visual",
				"icon": {
					"dark": "./media/view.png",
					"light": "./media/icon.png"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "resourceExtname == .html",
					"command": "visual.openVisual",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@types/vscode": "^1.67.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4"
	},
	"dependencies": {
		"dragselect": "^2.3.1",
		"jquery": "^3.6.0",
		"jQuery": "^1.7.4",
		"jsdom": "^19.0.0",
		"node-fetch": "^2.6.7"
	}
}
